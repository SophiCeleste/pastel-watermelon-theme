	"tokenColors": [
		{
			"scope": [
				"text.html.derivative entity.other.attribute-name", // html attribute names
				"source.css.scss entity.other.attribute-name", // scss class names
				"source.json support.type.property-name" // json properties
			],
			"settings": {
				"foreground": "#099866"
			}
		},
		{
			"scope": [
				"support.class.component.vue", // vue components
				"source.python entity.name.type.class" // python classes
			],
			"settings": {
				"foreground": "#099866",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"source.python entity.other.inherited-class", // python inherited classes
			],
			"settings": {
				"foreground": "#099866",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [],
			"settings": {
				"foreground": "#a8ea65"
			}
		},
		{
			"scope": [
				"text.html.derivative string", // html strings
			],
			"settings": {
				"foreground": "#a3f5a3"
			}
		},
		{
			"scope": [
				"source.css.scss support.type.property-name", // scss properties
				"source.css.scss meta.property-list.scss entity.name.tag.css" // additional scss properties
